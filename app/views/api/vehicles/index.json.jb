@vehicles.map do |vehicle|
  {
    id: vehicle.id,
    nickname: vehicle.nickname,
    make: vehicle.make,
    vehicle_model: vehicle.vehicle_model,
    year: vehicle.year,
    owner: vehicle.owner,
    color: vehicle.color,
    trim: vehicle.trim,
    image: vehicle.image,
    user_id: vehicle.user_id,
    user: vehicle.user.first_name,
  }
end
