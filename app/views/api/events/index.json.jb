@events.map do |event|
  {
    id: event.id,
    title: event.title,
    description: event.description,
    date: event.friendly_date,
    time: event.friendly_time,
    location: event.location,
    image: event.image,
    user_id: event.user_id,
    created_at: event.created_at,
    updated_at: event.updated_at,
  }
end
